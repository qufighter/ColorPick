      "run_at": "document_idle"
   } ],
   "background": {
      "scripts": ["options_prefs.js", "options_prefs_helpers.js", "background.js"],
      "persistent": false
   },
   "options_page": "options.html",
   "icons": {
       "16": "img/icon16.png",
       "32": "img/icon32.png",
       "48": "img/icon48.png",
       "64": "img/icon64.png",
      "128": "img/icon128.png"
