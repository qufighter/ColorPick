   }],
   "permissions": [
      "activeTab", "tabs", "scripting", "storage", "alarms"
   ],
   "browser_specific_settings": {"gecko": {"id": "jid1-kCS67LPIOiGf2Q@jetpack"}},
   "content_security_policy": {
	   "extension_pages": "script-src 'self'; object-src 'self'",
